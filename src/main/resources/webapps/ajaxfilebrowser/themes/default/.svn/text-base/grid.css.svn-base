/* ********************* */
/* Upload manager images */
/* ********************* */

.ih_vista .table .upload_status, .ih_vista .table .queue_status, .ih_vista .table .error_status {
	filter: expression(ITHit.WebDAV.Client.AjaxFileBrowser.Utils.FixPNG(this));
	width: 16px;
	height: 16px;
	margin: auto;
	background-repeat: no-repeat;
}

.ih_vista .table .upload_status {
	background-image: url('images/upload/upload.png');
}

.ih_vista .table .queue_status {
	background-image: url('images/upload/queue.png');
}

.ih_vista .table .error_status {
	background-image: url('images/upload/error.png');
}

.ih_vista .table .progress_bar {
	width: 98%;
	height: 6px;
	padding: 1px;
	border: 1px solid #CCCCCC;
	font-size: 0px;
	margin: auto;
	overflow: hidden; 
}

.ih_vista .table .progress {
	background-color: lightblue;
	height: 100%;
	width: 0%;
	font-size: 0px;
}

.ih_vista .table .complite {
	background-color: #879fae;
}

.ih_vista .table .error {
	background-color: red;
}





